{
    "compilerOptions": {
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "preserve",
        "module": "commonjs",
        "noEmit": true,
        "noImplicitAny": true,
        "paths": {
            "~test/*": ["./test/*"],
            "~usecases/*": ["./usecases/*"],
            "~*": ["./src/*"]
        },
        "skipLibCheck": true,
        "strict": true,
        "target": "es2017",
        "types": ["bun-types"]
    }
}
